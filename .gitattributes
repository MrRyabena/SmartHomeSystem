# linguist-vendored
# libraries/** linguist-vendored

* text=auto
* text eol=auto

# LF normalization
*.c text eol=lf
*.h text eol=lf
*.cpp text eol=lf
*.hpp text eol=lf
*.java text eol=lf
*.py text eol=lf
*.js text eol=lf
*.html text eol=lf
*.css text eol=lf
*.xml text eol=lf
*.json text eol=lf
*.md text eol=lf
*.txt text eol=lf
*.sh text eol=lf


# CRLF normalization
*.bat text eol=crlf
*.ini text eol=crlf


# binary files
*.png binary
*.jpg binary
*.jpeg binary
*.svg binary
*.gif binary
*.ico binary
*.mov binary
*.mp4 binary
*.mp3 binary
*.flv binary
*.fla binary
*.swf binary
*.gz binary
*.zip binary
*.7z binary
*.ttf binary
*.eot binary
*.woff binary
*.pyc binary
*.pdf binary
*.shsf binary
*.xmind binary
*.doc binary
*.pptx binary
